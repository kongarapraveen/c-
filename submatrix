#include<iostream>
#include<cmath>
using namespace std;
int main(){
    int a[3][3]={{1,2,3},
    {4,5,6},
    {7,8,9}};
    int submatrix[2][2];
    for(int i=0;i<2;i++){
        for(int j=0;j<2;j++){
            submatrix[i][j]=a[i][j];
        }
    }
    for(int i=0;i<2;i++){
        for(int j=0;j<2;j++){
            cout<<submatrix[i][j]<<" ";
        }
        cout<<endl;
    }
}
